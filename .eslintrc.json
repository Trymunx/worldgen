{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
      "indent": [
        "warn",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "semi": [
        "warn",
        "always"
      ],
      "no-unused-vars": "off",
      "no-undef": "off",
      "no-console": "off"
  }
}